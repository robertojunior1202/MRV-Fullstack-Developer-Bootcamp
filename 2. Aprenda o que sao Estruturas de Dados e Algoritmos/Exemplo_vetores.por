programa {
	funcao inicio() {
		//uma matriz só pode armazenar variaveis de um mesmo tipo, ou cadeia, ou caracter
		cadeia frutas[4] //vetor de 4 posicoes
		inteiro contador =0
		frutas[0]="Maçã"
		frutas[1]="Pera"
		frutas[2]="Laranja"
		frutas[3]="Uva"
		
		faca{
		escreva(frutas[contador]+"\n")
		contador++
		}enquanto (contador<=3)
		
	}
}
